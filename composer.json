{
  "name": "rossedlin/cryslo-php",
  "version": "2.0.0",
  "dist": {
    "url": "https://github.com/rossedlin/cryslo-php/archive/v2.0.0.zip",
    "type": "zip"
  },
  "require": {
    "php": ">=5.6.4",
    "swiftmailer/swiftmailer": "^5.4",
    "tijsverkoyen/css-to-inline-styles": "^2.2",
    "guzzlehttp/guzzle": "~6.0",
    "symfony/css-selector": "^3.3",
    "symfony/dom-crawler": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "5.5.*"
  },
  "autoload": {
    "psr-4": {
      "Cryslo\\Api\\": "src/api/",
      "Cryslo\\Core\\": "src/core/",
      "Cryslo\\Feed\\": "src/feed/"
    },
    "files": [
      "src/api/Api.php",
      "src/core/Core.php",
      "src/core/Helpers.php"
    ]
  }
}
