{
  "name": "rossedlin/cryslo-php-core",
  "version": "2.0.0",
  "dist": {
    "url": "https://github.com/rossedlin/cryslo-php-core/archive/v2.0.0.zip",
    "type": "zip"
  },
  "require": {
    "php": ">=5.6.4",
    "swiftmailer/swiftmailer": "^5.4",
    "tijsverkoyen/css-to-inline-styles": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "5.5.*"
  },
  "autoload": {
    "psr-4": {
      "Cryslo\\Core\\": "src/core/",
      "Cryslo\\Api\\": "src/api/",
      "Cryslo\\Feed\\": "src/feed/"
    },
    "files": [
      "src/Core.php",
      "src/Helpers.php"
    ]
  }
}
